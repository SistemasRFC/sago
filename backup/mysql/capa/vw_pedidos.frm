query=select straight_join `pp`.`QTD_NECESSARIA` AS `QTD_NECESSARIA`,`pp`.`TXT_OBSERVACAO` AS `TXT_OBSERVACAO`,`l`.`DSC_LOJA` AS `DSC_LOJA`,`l`.`DSC_LOJA_ABREVIADO` AS `DSC_LOJA_ABREVIADO`,`l`.`COD_LOJA` AS `COD_LOJA`,`pr`.`DSC_PRODUTO` AS `DSC_PRODUTO`,`p`.`IND_FECHADO` AS `IND_FECHADO`,`p`.`DATA_ENTREGA_PREVISTA` AS `DATA_ENTREGA_PREVISTA`,`p`.`COD_PERIODO_DIA` AS `COD_PERIODO_DIA`,`l`.`COD_CLIENTE` AS `COD_CLIENTE`,`d`.`COD_DEPARTAMENTO` AS `COD_DEPARTAMENTO`,`c`.`COD_CLASSIFICACAO` AS `COD_CLASSIFICACAO`,`d`.`DSC_DEPARTAMENTO` AS `DSC_DEPARTAMENTO`,`c`.`DSC_CLASSIFICACAO` AS `DSC_CLASSIFICACAO` from (((((`capa`.`re_pedido` `p` join `capa`.`re_pedido_produto` `pp` on(((`p`.`COD_PEDIDO` = `pp`.`COD_PEDIDO`) and (year(`p`.`DATA_PEDIDO`) = year(now()))))) join `capa`.`en_produto` `pr` on((`pp`.`COD_PRODUTO` = `pr`.`COD_PRODUTO`))) join `capa`.`en_classificacao` `c` on((`pr`.`COD_CLASSIFICACAO` = `c`.`COD_CLASSIFICACAO`))) join `capa`.`en_loja` `l` on((`p`.`COD_LOJA` = `l`.`COD_LOJA`))) join `capa`.`en_departamento` `d` on((`c`.`COD_DEPARTAMENTO` = `d`.`COD_DEPARTAMENTO`)))
